###  http://lisa.ehrlinger.cc/voc/dsd#AggregationAssociation
:AggregationAssociation rdf:type owl:Class ;
                        rdfs:subClassOf :Association ;
                        rdfs:comment "Describes the relationship between an aggregation that is composed out of several components."@en ;
                        rdfs:isDefinedBy :AggregationAssociation ;
                        rdfs:label "Aggregation association"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#Association
:Association rdf:type owl:Class ;
             rdfs:isDefinedBy :Association ;
             rdfs:label "Association"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#Attribute
:Attribute rdf:type owl:Class ;
           rdfs:comment "An Attribute describes a property of a Concept."@en ;
           rdfs:isDefinedBy :Attribute ;
           rdfs:label "Attribute"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#Concept
:Concept rdf:type owl:Class ;
         owl:hasKey ( :hasPrimaryKey
                    ) ;
         rdfs:comment "A Concept defines a part of a data source that can be e.g. a class in an object oriented source or a table in a relational database."@en ;
         rdfs:isDefinedBy :Concept ;
         rdfs:label "Concept"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#Datasource
:Datasource rdf:type owl:Class ;
            owl:disjointUnionOf ( :AggregationAssociation
                                  :Attribute
                                  :Concept
                                  :ForeignKey
                                  :InheritanceAssociation
                                  :PrimaryKey
                                  :ReferenceAssociation
                                ) ;
            rdfs:comment "A generic class for a data source. Could be anything from RDF-Triple Store, relational Datasource, comma separated list or just semi-structured text files."@en ;
            rdfs:isDefinedBy :Datasource ;
            rdfs:label "Datasource"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#DatasourceType
:DatasourceType rdf:type owl:Class ;
                rdfs:comment "Provides pre-defined Datasource Type Instances."@en ;
                rdfs:isDefinedBy :DatasourceType ;
                rdfs:label "Data model type"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#ForeignKey
:ForeignKey rdf:type owl:Class ;
            rdfs:comment "References to a Primary key and consists of one or more Attributes."@en ;
            rdfs:isDefinedBy :ForeignKey ;
            rdfs:label "Foreign key"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#InheritanceAssociation
:InheritanceAssociation rdf:type owl:Class ;
                        rdfs:subClassOf :Association ;
                        rdfs:comment "Describes the relationship between several child Concepts and their parent."@en ;
                        rdfs:isDefinedBy :InheritanceAssociation ;
                        rdfs:label "Inheritance association"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#PrimaryKey
:PrimaryKey rdf:type owl:Class ;
            rdfs:comment "Identifies a Concept and consists of one or more Attributes."@en ;
            rdfs:isDefinedBy :PrimaryKey ;
            rdfs:label "Primary key"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#ReferenceAssociation
:ReferenceAssociation rdf:type owl:Class ;
                      rdfs:subClassOf :Association ;
                      rdfs:comment "Describes a regular relationship between two or more Concepts."@en ;
                      rdfs:isDefinedBy :ReferenceAssociation ;
                      rdfs:label "Reference association"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#Stakeholder
:Stakeholder rdf:type owl:Class ;
             rdfs:comment "A stakeholder has access to a Datasource or a Concept of a Datasource. A stakeholder is typically a department of a company but can also be a person or a organization."@en ;
             rdfs:isDefinedBy :Stakeholder ;
             rdfs:label "Stakeholder"@en .


