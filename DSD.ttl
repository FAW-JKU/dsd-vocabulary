#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :AggregationAssociation
                :InheritanceAssociation
                :ReferenceAssociation
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Datasource
                :DatasourceType
                :Stakeholder
              )
] .


