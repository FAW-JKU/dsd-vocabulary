<http://lisa.ehrlinger.cc/voc/dsd> rdf:type owl:Ontology ;
                                    rdfs:comment "This document specifies a vocabulary for a generic description of a data source, its components and quality annotations."@en ;
                                    rdfs:isDefinedBy <http://lisa.ehrlinger.cc/voc/dsd> ;
                                    rdfs:label "Quality Annotations for Data Source Schemas"@en ;
                                    owl:versionInfo "2.0"^^xsd:double .

#################################################################
#    Object Properties
#################################################################

###  http://lisa.ehrlinger.cc/voc/dsd#consistsOfAttribute
:consistsOfAttribute rdf:type owl:ObjectProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( :ForeignKey
                                                 :PrimaryKey
                                               )
                                 ] ;
                     rdfs:range :Attribute ;
                     rdfs:comment "A Primary or Foreign Key consists of one or several Attributes."@en ;
                     rdfs:isDefinedBy :consistsOfAttribute ;
                     rdfs:label "consistsOfAttributes"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#hasAggregation
:hasAggregation rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasAssociationMember ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain :AggregationAssociation ;
                rdfs:range :Concept ;
                rdfs:comment "Describes the connection between an AggregationAssociation and its Aggregation." ;
                rdfs:isDefinedBy :hasAggregation ;
                rdfs:label "hasAggregation" .


###  http://lisa.ehrlinger.cc/voc/dsd#hasAggregationComponent
:hasAggregationComponent rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :hasAssociationMember ;
                         rdfs:domain :AggregationAssociation ;
                         rdfs:range :Concept ;
                         rdfs:comment "Describes the connection between an AggregationAssociation and one of its components." ;
                         rdfs:isDefinedBy :hasAggregationComponent ;
                         rdfs:label "hasAggregationComponent" .


###  http://lisa.ehrlinger.cc/voc/dsd#hasAssociationMember
:hasAssociationMember rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain :Association ;
                      rdfs:range :Concept ;
                      rdfs:comment "Describes the edges from an Association to its Members (Parent, Child, Aggregation, Component)."@en ;
                      rdfs:isDefinedBy :hasAssociationMember ;
                      rdfs:label "hasAssociationMember"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#hasAttribute
:hasAttribute rdf:type owl:ObjectProperty ;
              owl:inverseOf :isAttributeOf ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Association
                                          :Concept
                                        )
                          ] ;
              rdfs:range :Attribute ;
              rdfs:comment "Describes the number of Attributes a Concept or Association comprises."@en ;
              rdfs:isDefinedBy :hasAttribute ;
              rdfs:label "hasAttribute"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#hasChild
:hasChild rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasAssociationMember ;
          rdfs:domain :InheritanceAssociation ;
          rdfs:range :Concept ;
          rdfs:comment "Describes the connection between an InheritanceAssociation and one of its childs." ;
          rdfs:isDefinedBy :hasChild .


###  http://lisa.ehrlinger.cc/voc/dsd#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              owl:inverseOf :isComponentOf ;
              rdfs:domain :Datasource ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :Association
                                         :Attribute
                                         :Concept
                                         :ForeignKey
                                         :PrimaryKey
                                       )
                         ] ;
              rdfs:comment "Describes the number of components a Datasource comprises." ;
              rdfs:isDefinedBy :hasComponent ;
              rdfs:label "hasComponent" .


###  http://lisa.ehrlinger.cc/voc/dsd#hasForeignKey
:hasForeignKey rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Association
                                           :Concept
                                         )
                           ] ;
               rdfs:range :ForeignKey ;
               rdfs:isDefinedBy :hasForeignKey .


###  http://lisa.ehrlinger.cc/voc/dsd#hasParent
:hasParent rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasAssociationMember ;
           rdfs:domain :InheritanceAssociation ;
           rdfs:range :Concept ;
           rdfs:comment "Describes the connection between an InheritanceAssociation and a parent Concept." ;
           rdfs:isDefinedBy :hasParent ;
           rdfs:label "hasParent" .


###  http://lisa.ehrlinger.cc/voc/dsd#hasPrimaryKey
:hasPrimaryKey rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Association
                                           :Concept
                                         )
                           ] ;
               rdfs:range :PrimaryKey ;
               rdfs:isDefinedBy :hasPrimaryKey .


###  http://lisa.ehrlinger.cc/voc/dsd#isAttributeOf
:isAttributeOf rdf:type owl:ObjectProperty ;
               rdfs:domain :Attribute ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( :Association
                                          :Concept
                                        )
                          ] ;
               rdfs:comment "Describes an Attributes's connection to its Concept or Association."@en ;
               rdfs:isDefinedBy :isAttributeOf ;
               rdfs:label "isAttributeOf"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#isComponentOf
:isComponentOf rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Association
                                           :Attribute
                                           :Concept
                                           :ForeignKey
                                           :PrimaryKey
                                         )
                           ] ;
               rdfs:range :Datasource ;
               rdfs:comment "Describes a components connection to its Datasource."@en ;
               rdfs:isDefinedBy :isComponentOf ;
               rdfs:label "isComponentOf"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#isOfDataType
:isOfDataType rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ,
                       owl:AsymmetricProperty ;
              rdfs:domain :Attribute ;
              rdfs:comment "Describes an Attribute with an XLS Datatype."@en ;
              rdfs:isDefinedBy :isOfDataType ;
              rdfs:label "isOfDataType"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#isOfDatasourceType
:isOfDatasourceType rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ,
                             owl:AsymmetricProperty ;
                    rdfs:domain :Datasource ;
                    rdfs:range :DatasourceType ;
                    rdfs:comment "Describes the type of datasource a Datasource has. This Ontology provides some Individuals that can be used."@en ;
                    rdfs:isDefinedBy :isOfDatasourceType ;
                    rdfs:label "isOfDatasourceType"@en .


###  http://lisa.ehrlinger.cc/voc/dsd#providesAccess
:providesAccess rdf:type owl:ObjectProperty ,
                         owl:AsymmetricProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Association
                                            :Concept
                                            :Datasource
                                          )
                            ] ;
                rdfs:range :Stakeholder ;
                rdfs:comment "A datasource or part of a datasource (e.g. a concept) may provide access to one or more stakeholder."^^xsd:string ;
                rdfs:isDefinedBy :providesAccess ;
                rdfs:label "providesAccess"^^xsd:string .


###  http://lisa.ehrlinger.cc/voc/dsd#referencesFrom
:referencesFrom rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdf:type owl:AsymmetricProperty ;
                rdfs:domain :ReferenceAssociation ;
                rdfs:range :Concept ;
                rdfs:isDefinedBy :referencesFrom .


###  http://lisa.ehrlinger.cc/voc/dsd#referencesTo
:referencesTo rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Concept
                                          :PrimaryKey
                                          :ReferenceAssociation
                                        )
                          ] ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :Concept
                                         :ForeignKey
                                         :ReferenceAssociation
                                       )
                         ] ;
              rdfs:comment "A ForeignKey or Concept references to another Concept or a PrimaryKey."@en ;
              rdfs:isDefinedBy :referencesTo ;
              rdfs:label "referencesTo"@en .


